pkgname=qingy-themes
pkgver=1.0
pkgrel=1
pkgdesc="Themes Pack for qingy"
url='http://qingy.sourceforge.net'
depends=()
arch=('any')
license=('GPL')
source=(http://downloads.sourceforge.net/project/qingy/qingy%20themes/themepack%20for%20qingy%200.3xx/qingy_0.3_themepack_1.0.tar.bz2)
md5sums=('578413ba5861c667674b6d65976a0370')

build() {
  cd $srcdir/qingy_0.3_themepack_$pkgver

  mkdir -p $pkgdir/usr/share/qingy/themes

  mv ComputerRoom $pkgdir/usr/share/qingy/themes/computerroom
  mv aquaish $pkgdir/usr/share/qingy/themes/aquaish
  mv biohazard $pkgdir/usr/share/qingy/themes/biohazard
  mv casablanca $pkgdir/usr/share/qingy/themes/casablanca
  mv fireplace $pkgdir/usr/share/qingy/themes/fireplace
  mv frag $pkgdir/usr/share/qingy/themes/frag
  mv kitten $pkgdir/usr/share/qingy/themes/kitten
  mv lambretta $pkgdir/usr/share/qingy/themes/lambretta
  mv matrix $pkgdir/usr/share/qingy/themes/matrix
  mv vendetta $pkgdir/usr/share/qingy/themes/vendetta
  mv vendetta2 $pkgdir/usr/share/qingy/themes/vendetta2
  mv vendetta3 $pkgdir/usr/share/qingy/themes/vendetta3
}
